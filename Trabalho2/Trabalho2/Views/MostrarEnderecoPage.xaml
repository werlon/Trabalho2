<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Trabalho2.Views.MostrarEnderecoPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Endereço pelo cep"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />

            <Label Text="CEP"/>
            <Entry x:Name="Tcep" Placeholder="CEP"></Entry>

            <Label Text="Logradouro"/>
            <Entry x:Name="Tlogradouro"></Entry>

            <Label Text="Bairro"/>
            <Entry x:Name="Tbairro"></Entry>

            <Label Text="Cidade"/>
            <Entry x:Name="Tcidade"></Entry>

            <Label Text="UF"/>
            <Entry x:Name="Tuf"></Entry>


            <Label x:Name="lblResultado"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>